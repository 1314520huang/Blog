<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.netstudy.dao.FileMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.netstudy.bean.File">
        <id column="id" property="id" />
        <result column="upload_id" property="uploadId" />
        <result column="upload_name" property="uploadName" />
        <result column="file_id" property="fileId" />
        <result column="state" property="state" />
        <result column="create_time" property="createTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, upload_id, upload_name, file_id, state, create_time
    </sql>

</mapper>
